vetor = []

codigo = int(input("Informe o cÃ³digo 1 ou 2: "))

if codigo != 0 or codigo > 2 or codigo < 1:
    for n in range(0, 5):
        num = float(input("Informe um valor real: "))
        vetor.append(num)
        
    if codigo == 1:
        for n in vetor:
            print(n)
            
    if codigo == 2:
        vetor.reverse()
        for n in vetor:
            print(n)